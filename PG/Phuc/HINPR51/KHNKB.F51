Function KHNKB_Check(KHNKB)
'
    If Trim$(KHNKB) = "" Then
        KHNKB = "1"
    End If
    If KHNKB = "1" Or KHNKB = "9" Then
    Else
       KHNKB = "1"
    End If
       
    KHNKB_Check = 0
    
End Function

Function KHNKB_InitVal()
    '
    KHNKB_InitVal = "1"
End Function


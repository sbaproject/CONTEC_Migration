' === 20060801 === INSERT E -

    'äÑìñ≤›√ﬁØ∏ΩéÊìæ
    Trg_Index = CInt(pm_Ctl.Tag)

    Select Case True
        '¥›¿∞∑∞âü
        Case pm_KeyCode = vbKeyReturn And pm_Shift = 0
            pm_KeyCode = 0
            '¥›¿∞∑∞êßå‰
            Call Ctl_Item_VbKeyReturn(Main_Inf.Dsp_Sub_Inf(Trg_Index))


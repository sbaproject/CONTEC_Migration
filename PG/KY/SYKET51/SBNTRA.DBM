Global DB_SBNTRA As TYPE_DB_SBNTRA
Global DBN_SBNTRA As Integer
' Index1( DATNO )

Sub SBNTRA_RClear()
Dim TmpStat
    TmpStat = Dll_RClear(DBN_SBNTRA, G_LB)

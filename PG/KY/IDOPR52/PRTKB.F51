Attribute VB_Name = "PRTKB_F51"
Option Explicit
'
' スロット名        : 出力フラグ・画面項目スロット
' ユニット名        : PRTKB.F51
' 記述者            : Standard Library
' 作成日付          : 2006/08/22
' 使用プログラム名  : URIPR52
'

Function PRTKB_Check(PRTKB)
'
    If Trim$(PRTKB) = "" Then
        PRTKB = "0"
    End If
    If PRTKB = "0" Or PRTKB = "1" Or PRTKB = "9" Then
    Else
       PRTKB = "0"
    End If
       
    PRTKB_Check = 0
    
End Function

Function PRTKB_InitVal()
    '
    PRTKB_InitVal = 0
End Function

